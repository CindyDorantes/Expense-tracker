<h1>Expense Tracker</h1>
<% if user_signed_in? %>
  Logged in as <strong><%= current_user.email %></strong>.
  <%= link_to destroy_user_session_path, method: :delete, :class => 'navbar-link' do %>
    <button class="btn btn-primary mb-3">LOG OUT</button>
  <% end %>
<% else %>
  <%= link_to new_user_session_path, :class => 'navbar-link' do %>
    <button class="btn btn-primary mb-3">LOG IN</button>
  <% end %>
  <br>
  <%= link_to new_user_registration_path, :class => 'navbar-link' do %>
    <button class="btn btn-primary mb-3">SIGN UP</button>
  <% end %>
<% end %>